{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "description": "Template's website",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minhfi/tempalte/issues"
  },
  "homepage": "https://github.com/minhfi/tempalte",
  "keywords": [
    "reactjs",
    "react"
  ],
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 1 versions",
    "ie >= 11"
  ],
  "pre-commit": [
    "eslint",
    "test"
  ],
  "scripts": {
    "docker:up": "docker-compose --env-file ./.env up --no-start",
    "docker:start": "docker-compose start",
    "docker:restart": "docker-compose restart",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "predocker:start": "npm run docker:up",
    "start": "webpack-dev-server --config webpack/webpack.dev.js",
    "build": "webpack --mode=development --config webpack/webpack.prod.js",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "test": "node test.js --watchAll=false",
    "test-dev": "node test.js"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/material": "^5.9.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "connected-react-router": "^6.8.0",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-chained-backend": "^2.0.1",
    "i18next-http-backend": "^1.1.1",
    "i18next-localstorage-backend": "^3.1.1",
    "lodash": "^4.17.21",
    "qs": "^6.9.3",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.3.4",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-slick": "^0.28.1",
    "react-toastify": "^7.0.0-rc3",
    "react-transition-group": "^4.4.1",
    "react-tsparticles": "^1.37.4",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0",
    "slick-carousel": "^1.8.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.3",
    "babel-eslint": "^10.1.0",
    "babel-jest-glob-transform": "^1.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^10.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.1.1",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.9.0",
    "css-loader": "^3.4.2",
    "dotenv": "4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.1.0",
    "formdata-polyfill": "^3.0.20",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "interpolate-html-plugin": "^3.0.0",
    "is-wsl": "^2.1.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-resolve": "^26.6.2",
    "jest-watch-typeahead": "^0.6.1",
    "mime-types": "^2.1.30",
    "node-sass": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-safe-parser": "^5.0.2",
    "pre-commit": "^1.2.2",
    "promise.allsettled": "^1.0.4",
    "react-app-polyfill": "^2.0.0",
    "react-dev-utils": "^3.1.3",
    "react-error-overlay": "^1.0.7",
    "sass": "^1.32.8",
    "sass-loader": "^10.3.1",
    "style-loader": "^0.17.0",
    "svg-url-loader": "^5.0.0",
    "sw-precache-webpack-plugin": "^1.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-glob-loader": "^1.0.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@/static/(.*)$": "<rootDir>/public/static/$1",
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  }
}
